# Definition for singly-linked list.
class ListNode:
    def __init__(self, val=0, next=None):
        self.val = val
        self.next = next
class Solution:
    def isPalindrome(self, head: Optional[ListNode]) -> bool:
        arr=[]
        low=head
        while low is not None:
            arr.append(low.val)
            low=low.next
        n=len(arr)
        min=0
        max=n-1
        while min<max:
            if arr[min]!=arr[max]:
                return False
            min+=1
            max-=1
        return True






        
