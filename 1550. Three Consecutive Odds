class Solution:
    def threeConsecutiveOdds(self, arr: List[int]) -> bool:
        max_count=0
        n=len(arr)
        count=0
        for i in range(0,n):
            if  arr[i]%2==1:
                count+=1
                max_count=max(count,max_count)
            else:
                count=0
        if max_count>=3:
            return True
        else:
            return False     
